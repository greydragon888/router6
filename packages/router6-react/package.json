{
  "name": "router6-react",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "React integration for router6",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.mts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/esm/index.d.mts",
        "require": "./dist/cjs/index.d.ts"
      },
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/greydragon888/router6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greydragon888/router6.git"
  },
  "bugs": {
    "url": "https://github.com/greydragon888/router6/issues"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx modules/ tests/ --fix",
    "publint": "publint"
  },
  "keywords": [
    "router",
    "html5",
    "history",
    "tree",
    "react",
    "hooks",
    "components",
    "functional"
  ],
  "author": {
    "name": "Oleg Ivanov",
    "email": "greydragon888@gmail.com",
    "url": "https://github.com/greydragon888"
  },
  "license": "MIT",
  "sideEffects": false,
  "dependencies": {
    "router6": "workspace:^",
    "router6-helpers": "workspace:^"
  },
  "devDependencies": {
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "vitest-react-profiler": "1.12.0"
  },
  "peerDependencies": {
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "react": ">=18.0.0",
    "router6-plugin-browser": "workspace:^"
  }
}
