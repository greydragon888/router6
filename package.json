{
  "name": "router6",
  "type": "module",
  "description": "A real router. Just routing, nothing else.",
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:fast": "eslint --concurrency=auto --cache 'packages/*/modules/**/*.{ts,tsx}' 'packages/*/tests/**/*.{ts,tsx}'",
    "type-check": "turbo run type-check",
    "publint": "turbo run publint",
    "test:coverage": "turbo run test -- --coverage",
    "clean:all": "bash ./scripts/clean-all.sh",
    "clean:deep": "scripts/clean-deep.sh",
    "packages-structure": "find packages/*/modules -type f -name \"*.ts*\" -print | tree --fromfile"
  },
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greydragon888/router6.git"
  },
  "keywords": [
    "router",
    "routing",
    "reactive",
    "universal",
    "isomorphic",
    "framework-agnostic"
  ],
